{
    "name": "kento-oka/roust",
    "type": "library",
    "description": "Roust is the fastest (wishful) URI router",
    "keywords": [
        "router",
        "uri"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Kento Oka",
            "email": "oka.kento0311@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Roust\\": "src/"
        }
    },
    "scripts": {
        "test": [
            "composer test/parser",
            "composer test/router"
        ],
        "test/parser": "phpunit tests/ParserTest",
        "test/router": "phpunit tests/RouterTest"
    }
}
